<!DOCTYPE sqlMap
PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Axdoch">
	<select id="selectAxdoch" parameterClass="java.util.HashMap"
		resultClass="es.msssi.sgm.registropresencial.beans.ibatis.Axdoch">
		<isNotEmpty property="tableName">
			SELECT ID as id, FDRID as fdrid, NAME as name, CRTNDATE as crtndate FROM $tableName$ 
				WHERE ID = #id# AND FDRID = #fdrid# 
				ORDER BY CRTNDATE
		</isNotEmpty>
	</select>
	<delete id="deleteAxdoch" parameterClass="java.util.HashMap">
		<isNotEmpty property="tableNameDoc">
			DELETE FROM $tableNameDoc$ WHERE  ID = #docid# AND FDRID = #fdrid# 
		</isNotEmpty>
	</delete>
	
</sqlMap>