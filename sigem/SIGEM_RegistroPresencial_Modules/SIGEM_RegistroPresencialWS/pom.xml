	<build>
		<finalName>SIGEM_RegistroPresencialWS</finalName>
		<resources>
			<resource>
				<filtering>true</filtering>
				<directory>src/main/resources</directory>
			</resource>
			<resource>
				<filtering>true</filtering>
				<directory>${unpack.config.directory}</directory>
			</resource>
		</resources>

		<plugins>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.1-alpha-1</version>
				<configuration>
					<webXml>${basedir}/src/main/webapp/WEB-INF/web.xml</webXml>
					<webResources>
						<resource>
							<directory>src/main/webapp/WEB-INF</directory>
							<targetPath>WEB-INF</targetPath>
							<filtering>true</filtering>
						</resource>
					</webResources>
					<archive>
			            <manifest>
			              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
			            </manifest>
			            <manifestEntries>
			              <Implementation-Build>${buildNumber}</Implementation-Build>
			               <Weblogic-Application-Version>${project.version}</Weblogic-Application-Version>
			            </manifestEntries>
			          </archive>
				</configuration>
			</plugin>
			<!--
				Este fragmento descomprime el .zip de configurables
				que tenemos como dependecia (directa o transitiva)
				en el directorio que se indica en la property
				unpack.config.directory
			-->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>unpack-dependencies</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>unpack-dependencies</goal>
						</goals>
						<configuration>
							<includeClassifiers>configurableSigem</includeClassifiers>
							<includeTypes>zip</includeTypes>
							<excludeTypes>jar</excludeTypes>
							<type>zip</type>
							<outputDirectory>
								${unpack.config.directory}
							</outputDirectory>
							<overWriteReleases>false</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

	<profiles>
		<profile>
			<id>java2wsdl</id>
			<activation>
				<property>
					<name>java2wsdl</name>
				</property>
			</activation>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>axistools-maven-plugin</artifactId>
						<version>1.3</version>

						<configuration>
							<filename>ServicioRegistroWebService.wsdl</filename>
							<classOfPortType>
								ieci.tecdoc.sgm.registropresencial.ws.server.ServicioRegistroWebService
							</classOfPortType>
							<location>
								http://localhost:8080/sigem_registroPresencialWs/services/ServicioRegistroWebService
							</location>
							<use>literal</use>
						</configuration>
						<executions>
							<execution>
								<goals>
									<goal>java2wsdl</goal>
								</goals>
							</execution>
						</executions>
						<dependencies>
							<dependency>
								<groupId>axis</groupId>
								<artifactId>axis</artifactId>
								<version>1.3</version>
							</dependency>
						</dependencies>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>wsdl2java</id>
			<activation>
				<property>
					<name>wsdl2java</name>
				</property>
			</activation>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>axistools-maven-plugin</artifactId>
						<version>1.3</version>
						<configuration>
							<sourceDirectory>
								${basedir}/src/main/webapp/wsdl
							</sourceDirectory>
							<packageSpace>
								ieci.tecdoc.sgm.registropresencial.ws.client.axis
							</packageSpace>
							<serverSide>false</serverSide>
							<verbose>true</verbose>
							<testCases>false</testCases>
							<runTestCasesAsUnitTests>
								false
							</runTestCasesAsUnitTests>
							<typeMappingVersion>1.2</typeMappingVersion>
							<subPackageByFileName>false</subPackageByFileName>
							<outputDirectory>
								${basedir}/src/client/java
							</outputDirectory>
							<wrapArrays>false</wrapArrays>
						</configuration>
						<executions>
							<execution>
								<goals>
									<goal>wsdl2java</goal>
								</goals>
							</execution>
						</executions>
						<dependencies>
							<dependency>
								<groupId>axis</groupId>
								<artifactId>axis</artifactId>
								<version>1.3</version>
							</dependency>
						</dependencies>
					</plugin>
				</plugins>
			</build>
		</profile>
			
