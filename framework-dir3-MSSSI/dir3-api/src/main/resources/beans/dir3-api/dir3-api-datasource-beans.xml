<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="dir3_dataSourceImpl" name="dir3_dataSource"
		class="org.springframework.jndi.JndiObjectFactoryBean">
		<property name="jndiName" value="${weblogic.dataSource.jndi.name.fwktddir3DS}" />
	</bean>
	<bean id="registro_dataSourceImpl" name="registro_dataSource"
		class="org.springframework.jndi.JndiObjectFactoryBean">
		<property name="jndiName" value="${weblogic.dataSource.jndi.name.registroDS_000}" />
	</bean>
</beans>