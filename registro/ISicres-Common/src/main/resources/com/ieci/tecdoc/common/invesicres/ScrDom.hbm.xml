<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.ieci.tecdoc.common.invesicres.ScrDom" 
    table="SCR_DOM"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="SCR_DOM"
    </meta>

    <id
        name="id"
        type="java.lang.Integer"
        column="ID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="ID"

        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="address"
        type="java.lang.String"
        column="ADDRESS"
        not-null="true"
        length="100"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="ADDRESS"
            length="100"
            not-null="true"
        </meta>    
    </property>
    <property
        name="city"
        type="java.lang.String"
        column="CITY"
        length="40"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="CITY"
            length="40"
        </meta>    
    </property>
    <property
        name="zip"
        type="java.lang.String"
        column="ZIP"
        length="5"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="ZIP"
            length="5"
        </meta>    
    </property>
    <property
        name="country"
        type="java.lang.String"
        column="COUNTRY"
        length="30"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="COUNTRY"
            length="30"
        </meta>    
    </property>
    <property
        name="preference"
        type="int"
        column="PREFERENCE"
        not-null="true"
        length="10"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="PREFERENCE"
            length="10"
            not-null="true"
        </meta>    
    </property>

    <!-- associations -->

</class>
</hibernate-mapping>
