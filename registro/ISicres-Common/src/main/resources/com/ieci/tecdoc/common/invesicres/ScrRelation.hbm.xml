<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.ieci.tecdoc.common.invesicres.ScrRelation" 
    table="SCR_RELATIONS"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="SCR_RELATIONS"
    </meta>

    <composite-id>
        <meta attribute="class-description" inherit="false">
           @hibernate.id
            generator-class="assigned"
        </meta>
        <key-property 
            name="typebook" 
            column="TYPEBOOK" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="TYPEBOOK"
                length="10"
            </meta> 
        </key-property>
        <key-property 
            name="typerel" 
            column="TYPEREL" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="TYPEREL"
                length="10"
            </meta> 
        </key-property>
        <key-property 
            name="relyear" 
            column="RELYEAR" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="RELYEAR"
                length="10"
            </meta> 
        </key-property>
        <key-property 
            name="relmonth" 
            column="RELMONTH" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="RELMONTH"
                length="10"
            </meta> 
        </key-property>
        <key-property 
            name="relday" 
            column="RELDAY" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="RELDAY"
                length="10"
            </meta> 
        </key-property>
        <key-property 
            name="reldate" 
            column="RELDATE" 
            type="java.sql.Timestamp"
            length="7"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="RELDATE"
                length="7"
            </meta> 
        </key-property>
        <key-property 
            name="nrel" 
            column="NREL" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="NREL"
                length="10"
            </meta> 
        </key-property>
    </composite-id>    


    <!-- associations -->
    <!-- bi-directional many-to-one association to ScrOrg -->
    <many-to-one
        name="scrOrg"
        class="com.ieci.tecdoc.common.invesicres.ScrOrg"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="IDUNIT"         
        </meta>
        <column name="IDUNIT" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ScrOfic -->
    <many-to-one
        name="scrOfic"
        class="com.ieci.tecdoc.common.invesicres.ScrOfic"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="IDOFIC"         
        </meta>
        <column name="IDOFIC" />
    </many-to-one>

</class>
</hibernate-mapping>
