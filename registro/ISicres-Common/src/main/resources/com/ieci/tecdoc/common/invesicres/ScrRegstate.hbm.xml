        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="state"
        type="int"
        column="STATE"
        not-null="true"
        length="10"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="STATE"
            length="10"
            not-null="true"
