-->

<class 
    name="com.ieci.tecdoc.common.invesicres.ScrTypeaddress" 
    table="SCR_TYPEADDRESS"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="SCR_TYPEADDRESS"
    </meta>

    <id
        name="id"
        type="java.lang.Integer"
        column="ID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="ID"

        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="description"
        type="java.lang.String"
        column="DESCRIPTION"
        not-null="true"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="DESCRIPTION"
            length="50"
            not-null="true"
        </meta>    
    </property>
    <property
        name="code"
        type="java.lang.String"
        column="CODE"
        not-null="true"
        length="2"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="CODE"
            length="2"
            not-null="true"
        </meta>    
    </property>

    <!-- associations -->
    <!-- bi-directional one-to-many association to ScrAddrtel -->
    <set
        name="scrAddrtels"
        lazy="true"
        inverse="true"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"

	       @hibernate.collection-key
	        column="TYPE"

           @hibernate.collection-one-to-many
            class="com.ieci.tecdoc.common.invesicres.ScrAddrtel"
        </meta>
        <key>
            <column name="TYPE" />
        </key>
        <one-to-many 
            class="com.ieci.tecdoc.common.invesicres.ScrAddrtel"
        />
    </set>

</class>
</hibernate-mapping>
